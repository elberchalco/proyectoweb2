<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

<servlet>
		<servlet-name>Tienda_Onli</servlet-name>
		<servlet-class>tienda.online.Tienda_OnliServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Tienda_Onli</servlet-name>
		<url-pattern>/tienda_onli</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Registro</servlet-name>
		<servlet-class>tienda.online.Registro</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registro</servlet-name>
		<url-pattern>/registro</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>tienda.online.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>CerrarSesion</servlet-name>
		<servlet-class>tienda.online.CerrarSesion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CerrarSesion</servlet-name>
		<url-pattern>/cerrarSesion</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CerrarAdmin</servlet-name>
		<servlet-class>tienda.online.CerrarAdmin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CerrarAdmin</servlet-name>
		<url-pattern>/cerrarAdmin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Remove</servlet-name>
		<servlet-class>tienda.online.Remove</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Remove</servlet-name>
		<url-pattern>/remove</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Modificar</servlet-name>
		<servlet-class>tienda.online.Modificar</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Modificar</servlet-name>
		<url-pattern>/modificar</url-pattern>
	</servlet-mapping>
	
	
	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
